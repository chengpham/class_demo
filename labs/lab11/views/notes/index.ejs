<%- include('../partials/header') %>
<h1>Notes Index</h1>
<div>
    <% for (let note of notes) { %>
        <div class='card mb-4'>
            <div class='card-body'>
                <ul>
                    <li>
                    <p class='card-test'> <%= note.content%> </p>
                    </li>
                </ul>
                <a class='btn btn-outline-success' href='/notes/<%= note.id %>/edit'>Edit</a>
            </div>
        </div>
    <% } %>
    <a class='btn btn-outline-success' href='/notes/new'>Create New Notes</a>
</div>

<%- include('../partials/footer') %>